            android:padding="10dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:src="@drawable/release_road_show"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/my_item_height"
            android:drawableRight="@drawable/ic_setting_arrow"
            android:gravity="center_vertical"
            android:paddingRight="14dp"
            android:text="我的路演"
            android:textColor="@color/text_color_black"
            android:textSize="@dimen/text_size_16" />
            />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@drawable/list_view_divider"/>
    <LinearLayout
        android:id="@+id/ll_my_launch"
        android:background="@drawable/list_item_selector"
        android:layout_width="match_parent"
        android:layout_height="@dimen/my_item_height">

        <ImageView
            android:layout_marginLeft="10dp"
            android:padding="10dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:src="@drawable/my_launch"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/my_item_height"
            android:drawableRight="@drawable/ic_setting_arrow"
            android:gravity="center_vertical"
            android:paddingRight="14dp"
            android:text="我的发起"
            android:textColor="@color/text_color_black"
            android:textSize="@dimen/text_size_16" />
            />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@drawable/list_view_divider"/>
    <LinearLayout
        android:layout_marginTop="20dp"
        android:id="@+id/ll_version_upgrade"
        android:background="@drawable/list_item_selector"
        android:layout_width="match_parent"
        android:layout_height="@dimen/my_item_height">

        <ImageView
